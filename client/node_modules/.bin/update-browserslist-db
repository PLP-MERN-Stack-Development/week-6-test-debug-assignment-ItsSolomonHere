#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/solomon-nyakwama/Desktop/week-6-test-debug-assignment-ItsSolomonHere/client/node_modules/update-browserslist-db/node_modules:/home/solomon-nyakwama/Desktop/week-6-test-debug-assignment-ItsSolomonHere/client/node_modules:/home/solomon-nyakwama/Desktop/week-6-test-debug-assignment-ItsSolomonHere/node_modules:/home/solomon-nyakwama/Desktop/node_modules:/home/solomon-nyakwama/node_modules:/home/node_modules:/node_modules:/home/solomon-nyakwama/Desktop/week-6-test-debug-assignment-ItsSolomonHere/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/solomon-nyakwama/Desktop/week-6-test-debug-assignment-ItsSolomonHere/client/node_modules/update-browserslist-db/node_modules:/home/solomon-nyakwama/Desktop/week-6-test-debug-assignment-ItsSolomonHere/client/node_modules:/home/solomon-nyakwama/Desktop/week-6-test-debug-assignment-ItsSolomonHere/node_modules:/home/solomon-nyakwama/Desktop/node_modules:/home/solomon-nyakwama/node_modules:/home/node_modules:/node_modules:/home/solomon-nyakwama/Desktop/week-6-test-debug-assignment-ItsSolomonHere/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../update-browserslist-db/cli.js" "$@"
else
  exec node  "$basedir/../update-browserslist-db/cli.js" "$@"
fi
